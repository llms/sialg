with
member [Measures].[Defeitos] as 'IIF([Measures].[Defeito Criado em PRD]>0,([Measures].[Qt. Pontos Funcao de DMS ou DNS Implantadas]/[Measures].[Defeito Criado em PRD]),0)'
set [Rows] as FILTER(
[Organizacao.Gestao da Demanda].[Sigla GEA].MEMBERS
, [Organizacao.Gestao da Demanda].[Sigla GEA].currentMember <> [Organizacao.Gestao da Demanda].[SUCTI].[NI] )
SET [SLICER] AS 'NONEMPTYCROSSJOIN([Rows],[Filtro])'
SET [SLICER_AXIS] AS 'GENERATE([SLICER], {([Data Resolucao Pai].CURRENTMEMBER)})'

SET [Filtro] AS 'FILTER(descendants([Data Resolucao Pai].MEMBERS, [Data Resolucao Pai].[Data de Resolucao Pai (D)]), ( [Data Resolucao Pai].CURRENTMEMBER IN ({[Data Resolucao Pai].[2016].[4].[10].[Outubro].[2016-10-01 00:00:00.0]:[Data Resolucao Pai].[2016].[4].[12].[Dezembro].[2016-12-28 00:00:00.0]}))) * FILTER(descendants([Data Resolucao Pai].MEMBERS, [Data Resolucao Pai].[Data de Resolucao Pai (D)]), ( [Data Resolucao Filha].CURRENTMEMBER IN ({[Data Resolucao Filha].[2016].[4].[10].[Outubro].[2016-10-01 00:00:00.0]:[Data Resolucao Filha].[2016].[4].[12].[Dezembro].[2016-12-28 00:00:00.0]}))) '

SELECT
[Rows] ON ROWS
,[Measures].[Defeitos] ON COLUMNS
FROM [SIALG - Demandas]  where ([SLICER_AXIS])
